// Styleguide Icons

/*
Généré par Gulp

Commande : gulp fonticon
*/

/* ==========================================================================
Font
========================================================================== */
@font-face {
font-family: "{{ fontName }}";
font-weight: normal;
font-style: normal;
src: url("../../fonts/{{ fontName }}.woff2?v={{ fontVersion }}") format("woff2"),
url("../../fonts/{{ fontName }}.woff?v={{ fontVersion }}") format("woff");
}

@function font-icon-name() {
@return "{{ fontName }}";
}

/*
{% for glyph in glyphs %}
    .icon--{{ glyph.name }} - \{{ glyph.unicode }}
{% endfor %}

Markup: <span class="icon $modifierClass"></span>

Styleguide Icons - 1.icon
*/
{% for glyph in glyphs %}
    .icon--{{ glyph.name }}:before {
    content: "\{{ glyph.unicode }}";
    }
{% endfor %}
